<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.control.Slider?>

<BorderPane prefHeight="350.0" prefWidth="350"
	xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8"
	fx:controller="eventHandlers.BindingController">
	<top>
		<VBox prefHeight="170.0" prefWidth="350.0"
			BorderPane.alignment="CENTER" spacing="10">
			<children>
				<Label text="textArea1" />
				<TextArea fx:id="textArea1" prefHeight="69.0"
					prefWidth="350.0" />
				<Label text="textArea2" />
				<TextArea fx:id="textArea2" prefHeight="76.0"
					prefWidth="350.0" />
			</children>
		</VBox>
	</top>
	<center>
		<Label fx:id="label" text="JavaFX">
			<font>
				<Font size="0" />
			</font>
		</Label>
	</center>
	<bottom>
		<Slider fx:id="slider" />
	</bottom>
</BorderPane>
