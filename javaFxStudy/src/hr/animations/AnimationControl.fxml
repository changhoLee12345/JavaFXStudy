<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.StackPane?>


<StackPane prefHeight="500.0" prefWidth="350.0" fx:controller="animations.AnimationController"
	xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8">
	<children>
		<BorderPane prefHeight="200.0" prefWidth="200.0">
			<top>
				<BorderPane prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: #eaeaea;" BorderPane.alignment="CENTER">
					<center>
						<Label prefWidth="215.0" text="메인화면" BorderPane.alignment="CENTER" />
					</center>
					<right>
						<Button fx:id="btnLogin" mnemonicParsing="false" text="로그인" BorderPane.alignment="CENTER">
						</Button>
					</right>
					<padding>
						<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
					</padding>
				</BorderPane>
			</top>
			<center>
				<VBox prefHeight="200.0" prefWidth="100.0" style="-fx-background-color: #0000ff;" />
			</center>
		</BorderPane>
	</children>
</StackPane>
