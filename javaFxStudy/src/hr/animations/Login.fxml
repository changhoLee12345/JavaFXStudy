<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>


<BorderPane fx:id="login" prefHeight="500.0" prefWidth="350.0" xmlns:fx="http://javafx.com/fxml/1"
	xmlns="http://javafx.com/javafx/8" fx:controller="animations.LoginController">
	<top>
		<BorderPane prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: #eaeaea;" BorderPane.alignment="CENTER">
			<left>
				<Button fx:id="btnMain" mnemonicParsing="false" text="메인" BorderPane.alignment="CENTER" />
			</left>
			<center>
				<Label prefWidth="215.0" text="로그인화면" BorderPane.alignment="CENTER" />
			</center>
			<padding>
				<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
			</padding>
		</BorderPane>
	</top>
	<center>
		<VBox prefHeight="200.0" prefWidth="100.0" style="-fx-background-color: #ffff00;" BorderPane.alignment="CENTER" />
	</center>
</BorderPane>
