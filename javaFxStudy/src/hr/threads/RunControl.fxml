<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="100.0" prefWidth="200.0" fx:controller="threads.RunController"
	xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8">
	<children>
		<Label fx:id="lblTime" alignment="CENTER" layoutX="25.0" layoutY="15.0" prefHeight="35.0" prefWidth="150.0"
			style="-fx-background-color: black; -fx-text-fill: yellow; -fx-font-size: 20; -fx-background-radius: 10;"
			text="00:00:00" />
		<Button fx:id="btnStart" layoutX="110.0" layoutY="63.0" mnemonicParsing="false" text="시작" />
		<Button fx:id="btnStop" layoutX="46.0" layoutY="63.0" mnemonicParsing="false" text="멈춤" />
	</children>
</AnchorPane>
